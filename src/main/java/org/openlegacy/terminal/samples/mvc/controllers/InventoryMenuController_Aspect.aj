privileged @SuppressWarnings("unused") aspect InventoryMenuController_Aspect {

	@Inject
	private TerminalSession InventoryMenuController.terminalSession;

	@Inject
	private ScreenEntitiesRegistry InventoryMenuController.screenEntitiesRegistry;

	// handle page initial display
    @RequestMapping(method = RequestMethod.GET)
    public String InventoryMenuController.show(Model uiModel) {
    	InventoryMenu InventoryMenu = terminalSession.getEntity(InventoryMenu.class);
	uiModel.addAttribute("inventoryMenu", InventoryMenu);
	// show the resulting page for InventoryMenu
        return "InventoryMenu";
    }

	// handle submit action
    @RequestMapping(method = RequestMethod.POST)
    public String InventoryMenuController.submit(InventoryMenu inventoryMenu, Model uiModel, HttpServletRequest httpServletRequest) {
	ScreenEntity resultScreenEntity = terminalSession.doAction(TerminalActions.ENTER(), inventoryMenu);
	// go to the controller for the resulting screen name
		if (resultScreenEntity != null){
			String screenEntityName = screenEntitiesRegistry.get(resultScreenEntity.getClass()).getEntityClassName();
			if (httpServletRequest.getParameter("partial") != null){
				inventoryMenu = terminalSession.getEntity(InventoryMenu.class);
		    	uiModel.addAttribute("inventoryMenu", inventoryMenu);
		        return "InventoryMenu";
			}
        	return "redirect:" + screenEntityName;
		}
    	return "redirect:/";
		
    }
    
